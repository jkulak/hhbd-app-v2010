      <announced>
        <route>zapowiedziane.html</route>
        <defaults>
          <controller>album</controller>
          <action>announced</action>
        </defaults>
      </announced>

      <album type="Zend_Controller_Router_Route_Regex">
        <route>(.+)-a(\d+).html</route>
        <defaults>
          <controller>album</controller>
          <action>view</action>
        </defaults>
        <map>
          <seo>1</seo>
          <id>2</id>
        </map>
        <reverse>%s-a%d.html</reverse>
      </album>

      <artists>
         <route>wykonawcy.html</route>
         <defaults>
           <controller>artist</controller>
           <action>index</action>
         </defaults>
         <reverse>wykonawcy.html</reverse>
       </artists>

       <artist type="Zend_Controller_Router_Route_Regex">
         <route>(.+)-p(\d+).html</route>
         <defaults>
           <controller>artist</controller>
           <action>view</action>
         </defaults>
         <map>
           <seo>1</seo>
           <id>2</id>
         </map>
         <reverse>%s-p%d.html</reverse>
       </artist>

      <labels>
          <route>wytwornie.html</route>
          <defaults>
            <controller>label</controller>
            <action>index</action>
          </defaults>
          <reverse>wytwornie.html</reverse>
      </labels>

        <label type="Zend_Controller_Router_Route_Regex">
          <route>wytwornia/(.+)-(\d+).html</route>
          <defaults>
            <controller>label</controller>
            <action>view</action>
          </defaults>
          <map>
            <seo>1</seo>
            <id>2</id>
          </map>
          <reverse>wytwornia/%s-%d.html</reverse>
        </label>

        <song type="Zend_Controller_Router_Route_Regex">
          <route>hip-hop/(.+)-(\d+).html</route>
          <defaults>
            <controller>song</controller>
            <action>view</action>
          </defaults>
          <map>
            <seo>1</seo>
            <id>2</id>
          </map>
          <reverse>hip-hop/%s-%d.html</reverse>
        </song>
        
    </routes>
  </production>
  <development extends="production">
    
  </development>
</configdata>
