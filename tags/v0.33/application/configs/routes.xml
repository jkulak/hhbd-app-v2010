<?xml version="1.0" encoding="UTF-8"?>
<configdata>
  <production>
    <routes>
      
      <homeSite type="Zend_Controller_Router_Route_Static">
        <route/>
        <defaults>
          <controller>index</controller>
          <action>index</action>
        </defaults>
        <reverse/>
      </homeSite>
      
      <aboutUs>
        <route>o-nas.html</route>
        <defaults>
          <controller>index</controller>
          <action>aboutUs</action>
        </defaults>
      </aboutUs>
      
      <top10>
        <route>top10.html</route>
        <defaults>
          <controller>top</controller>
          <action>index</action>
        </defaults>
        <reverse>/top10.html</reverse>
      </top10>
      
<!-- Albumy Wykonawcy Wytwórnie "O nas" Blog Kontakt "Zasady użytkowania" "Mapa strony" -->
      <!-- articles -->
      
      <albums>
        <route>albumy.html</route>
        <defaults>
          <controller>album</controller>
          <action>index</action>
        </defaults>
      </albums>
      
